                },
                "is_active": {
                    "type": "boolean"
                },
                "last_status": {
                    "type": "integer"
                },
                "latency_ms": {
                    "type": "integer"
                },
                "url": {
                    "type": "string"
                },
                "user_id": {
                    "type": "integer"
                }
