                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "sites"
                ],
                "summary": "Create a new site by user",
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
